{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/((?!api|_next/static|_next/image|assets|favicon.ico|manifest.json|robots.txt|sitemap.xml|sw.js).*)",
      "destination": "/index.html"
    }
  ]
}
